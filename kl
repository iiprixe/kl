local B = C.Slider({
    Text = "ws/walkspeed",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
    end,
    Min = 16,
    Max = 200,
    Def = 16
})
local B = C.Slider({
    Text = "jp/jumppower",
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
    end,
    Min = 16,
    Max = 200,
    Def = 16
})
local B = M.Slider({
    Text = "Cash/Coins",
    Callback = function(Value)
        game.Players.LocalPlayer.leaderstats.Coins.Value = value
    end,
    Min = 0,
    Max = 1000000000000,
    Def = 0
})

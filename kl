getgenv().autoegg = false;
local Material = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/MaterialLua/master/Module.lua"))()

local X = Material.Load({
    Title = "[Stormzzys Scripts] </> Drone Swarm Sim",
    Style = 1,
    SizeX = 370,
    SizeY = 300,
    Theme = "Dark", --light dark aqua mocha jester
    ColorOverrides = {
		NavBarAccent = Color3.fromRGB(130,130,130)
    }
})

local Y = X.New({
    Title = "Eggs"
})

local Z = X.New({
    Title = "Teleports"
})

local R = X.New({
    Title = "Bosses"
})

local C = X.New({
    Title = "Character"
})

local M = X.New({
    Title = "Misc"
})

local B = Y.Toggle({
    Text = "Open Starter Capsule",
    Callback = function(Value)
        getgenv().autoegg = bool
        
        spawn(function()
        while autoegg == true do
local ohString1 = "Starter Capsule"
local ohBoolean2 = false
local ohBoolean3 = false

game:GetService("ReplicatedStorage").Remotes.OpenEgg:InvokeServer(ohString1, ohBoolean2, ohBoolean3)
 		wait()
 	 end
 end)
    end,
    Enabled = false
})

